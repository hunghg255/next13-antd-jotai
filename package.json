{
  "name": "next-13-antd-jotai",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.clientDev next dev",
    "build": "next build",
    "buildDev": "npm run prebuild && cp -f .env.clientDev .env && next build",
    "buildProd": "cp -f .env.clientProd .env && next build",
    "start": "env-cmd -f .env.clientProd next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "predev": "ts-node --project ./tsconfig.node.json ./scripts/genAntdCss.tsx",
    "prebuild": "cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./scripts/genAntdCss.tsx",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss",
    "standalone": "node server.js"
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "antd": "^5.10.3",
    "autoprefixer": "^10.4.16",
    "brainless-token-manager": "^1.3.3",
    "classnames": "^2.4.0",
    "cookies-next": "^4.1.0",
    "dayjs": "^1.11.10",
    "jotai": "^2.6.0",
    "next": "^14.0.1",
    "next-i18next": "^11.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.1",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.2",
    "@hunghg255/eslint-config-react": "^0.0.26",
    "@next/bundle-analyzer": "^12.3.4",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "18.7.16",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "bumpp": "^9.2.1",
    "cross-env": "^7.0.3",
    "csvgtocss": "^0.0.8",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.5.6",
    "git-scm-hooks": "^0.0.11",
    "next-hydration-overlay": "^0.0.17",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "4.8.3",
    "verify-commit-msg": "^0.0.14"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
